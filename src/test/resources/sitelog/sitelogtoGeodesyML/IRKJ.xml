         <geo:monumentDescription>PILLAR</geo:monumentDescription>
         <geo:heightOfTheMonument>2.5</geo:heightOfTheMonument>
         <geo:monumentFoundation>CONCRETE BLOCK</geo:monumentFoundation>
         <geo:foundationDepth>2.7</geo:foundationDepth>
